env:
    browser: true
    es2021: true
extends:
    - standard
    - prettier
parser: '@typescript-eslint/parser'
parserOptions:
    ecmaVersion: 12
    sourceType: module
plugins:
    - '@typescript-eslint'
    - 'prettier'
    - 'import'
rules:
    import/newline-after-import: error
    newline-before-return: error
    "prettier/prettier": error
    import/order:
        - error
        -
            newlines-between: always

